{
  "private": true,
  "author": "wilderzorn <wilderzorn@163.com>",
  "name": "maxweb",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "cross-env UMI_ENV=release max build",
    "build:dev": "cross-env UMI_ENV=dev max build",
    "commitlint": "commitlint --edit",
    "dev": "cross-env CLIENT_ENV=dev max dev",
    "format": "prettier --cache --write .",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "preview": "max preview",
    "setup": "max setup",
    "start": "cross-env CLIENT_ENV=pre max dev",
    "lint:js": "umi lint --eslint-only",
    "lint:fix": "umi lint --fix",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "umi lint --stylelint-only",
    "lint:cssinjs": "umi lint --stylelint-only --cssinjs",
    "precommit:fix": "npm run lint:fix && npm run prettier && npm run lint:prettier && npm run lint:style",
    "prettier": "prettier -c --write \"src/**/*\"",
    "lint":"max setup && npm run lint:js && npm run lint:style && npm run lint:prettier"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "eslint --ext .js,.jsx,.ts,.tsx src --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@ant-design/pro-components": "^2.4.4",
    "@antv/g6": "4.8.24",
    "@umijs/max": "^4.2.5",
    "ahooks": "^3.7.11",
    "antd": "^5.4.0",
    "antd-style": "^3.6.2",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.11",
    "events": "^3.3.0",
    "lodash": "^4.17.21",
    "rc-notification": "^5.5.0",
    "rc-tween-one": "^3.0.6",
    "react-virtualized": "^9.22.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/lodash": "^4.17.6",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/react-virtualized": "^9.21.30",
    "cross-env": "^7.0.3",
    "eslint": "8.49.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "typescript": "5.0.3"
  },
  "volta": {
    "node": "20.15.1"
  }
}
