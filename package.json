{
  "private": true,
  "author": "wilderzorn <wilderzorn@163.com>",
  "name": "maxweb",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "cross-env UMI_ENV=release max build",
    "build:dev": "cross-env UMI_ENV=dev max build",
    "commitlint": "commitlint --edit",
    "format": "prettier --cache --write .",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "preview": "max preview",
    "setup": "max setup",
    "start:dev": "cross-env CLIENT_ENV=dev max dev",
    "start": "cross-env CLIENT_ENV=pre max dev",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:prettier:fix": "prettier -c --write \"src/**/*\"",
    "lint": "max lint && npm run lint:prettier",
    "lint:fix": "max lint --fix && npm run lint:prettier:fix",
    "precommit:fix": "npm run lint:fix",
    "clean": "npm cache clean -f"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,less}": [
      "max lint"
    ]
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@ant-design/icons": "^5.5.1",
    "@ant-design/pro-components": "^2.7.19",
    "@antv/g6": "4.8.24",
    "@antv/l7": "2.20.11",
    "@antv/l7-district": "^2.3.12",
    "@antv/l7-maps": "2.20.11",
    "@umijs/max": "^4.4.6",
    "ahooks": "^3.8.1",
    "antd": "^5.24.2",
    "antd-style": "^3.7.0",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "echarts": "5.1.2",
    "emittery": "^1.0.3",
    "es-toolkit": "^1.24.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "rc-notification": "^5.6.2",
    "rc-tween-one": "^3.0.6",
    "react": "^18.3.1",
    "react-virtualized": "^9.22.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/lodash": "^4.17.10",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-virtualized": "^9.21.30",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.5.2",
    "typescript": "5.0.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": ">=20"
  },
  "volta": {
    "node": "20.18.3"
  }
}
